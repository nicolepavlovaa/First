#include <iostream>
#include <cmath>
using namespace std;
int main()
{
	int n;
	double nominator = 1, denominator = 0, number, sum=0;
	cin >> n;
	for (int i = 0; i < n; i++)
	{
		for (int j = 1; j <= n; j++)
		{
			nominator = nominator * j;
		}
		for (int j = 2; j <= n + 1; j++)
		{
			denomimnator = denominator + (1 / (double)j);
		}
		number = nominator / denominator;
		sum = sum + number;
	}
	cout << sum << endl;

	system("pause");
	return 0;
}
